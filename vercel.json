{
  "rewrites": [
    {
      "source": "/api/v4/(?<path>.*)",
      "destination": "https://api.football-data.org/v4/$path"
    }
  ],
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist/frontend/browser"
      }
    }
  ]
}